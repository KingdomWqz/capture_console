{
    "name": "CaptureRequest",
    "short_name": "CaptureRequest",
    "description": "Capture HTTP Request using Chrome Extension V3",
    "version": "0.0.2",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "debugger"
    ]
}